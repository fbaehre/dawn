          "settings": {
            "custom_liquid": "<div class=\"product-form__input product-form__quantity\" {{ block.shopify_attributes }}>\n<label class=\"form__label\" for=\"Quantity-{{ section.id }}\">\n{{ 'products.product.quantityMeter.label' | t }}<\/label>\n<quantity-input class=\"quantity\">\n<button class=\"quantity__button no-js-hidden\" name=\"minus\" type=\"button\">\n<span class=\"visually-hidden\">{{ 'products.product.quantity.decrease' | t: product: product.title | escape }}<\/span>\n{% render 'icon-minus' %}\n<\/button>\n<input class=\"quantity__input\"                     type=\"number\"                   name=\"quantity\"           id=\"Quantity-{{ section.id }}\"\n                      min=\"3\"                     value=\"3\"                      form=\"product-form-{{ section.id }}\"\n>\n\n<button class=\"quantity__button no-js-hidden\" name=\"plus\" type=\"button\">\n\n<span class=\"visually-hidden\">\n{{ 'products.product.quantity.increase' | t: product: product.title | escape }}<\/span>\n{% render 'icon-plus' %}\n\n<\/button>\n<\/quantity-input>\n<\/div>"
          }
